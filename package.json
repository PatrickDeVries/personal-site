{
  "name": "personal-site",
  "version": "0.1.0",
  "homepage": "http://patrickdevries.com/",
  "private": true,
  "scripts": {
    "predeploy": "yarn lint && yarn build",
    "preview": "vite preview",
    "deploy": "yarn predeploy && gh-pages -d dist",
    "start": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:code": "eslint 'src/**/*.{ts,tsx}'",
    "lint:styles": "stylelint 'src/**/*.{ts,tsx}'",
    "lint:formatting": "prettier --check src",
    "lint:types": "tsc --noEmit",
    "lint": "yarn lint:code && yarn lint:styles && yarn lint:formatting && yarn lint:types"
  },
  "dependencies": {
    "@mdi/js": "^6.9.96",
    "@mdi/react": "^1.6.1",
    "@react-three/fiber": "^8.9.1",
    "babel-plugin-styled-components": "^2.0.7",
    "gh-pages": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "polished": "^4.2.2",
    "prettier-plugin-organize-imports": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "styled-components": "^5.3.6",
    "three": "^0.140.2",
    "typescript": "^4.4.2",
    "valtio": "^1.6.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.137.0",
    "@typescript-eslint/parser": "^5.45.1",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.29.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "postcss": "^8.4.19",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "vite": "^3.2.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.3.0"
}
